{
  "framework": "nextjs",
  "buildCommand": "bash build-vercel.sh",
  "outputDirectory": ".next",
  "installCommand": "npm ci --silent 2>/dev/null || npm install --silent",
  "devCommand": "npm run dev",
  "env": {
    "NPM_CONFIG_AUDIT": "false",
    "NPM_CONFIG_FUND": "false",
    "NPM_CONFIG_LOGLEVEL": "silent",
    "NPM_CONFIG_PROGRESS": "false",
    "NPM_CONFIG_UPDATE_NOTIFIER": "false",
    "NPM_CONFIG_OPTIONAL": "false",
    "NPM_CONFIG_WARNINGS": "false",
    "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
    "DISABLE_OPENCOLLECTIVE": "true",
    "ADBLOCK": "true",
    "NODE_OPTIONS": "--no-deprecation --no-warnings --disable-warning=ExperimentalWarning --max-old-space-size=4096",
    "NODE_NO_WARNINGS": "1",
    "CI": "true",
    "SUPPRESS_NO_CONFIG_WARNING": "true",
    "NO_UPDATE_NOTIFIER": "true",
    "NEXT_TELEMETRY_DISABLED": "1",
    "HUSKY": "0"
  },
  "functions": {
    "app/**": {
      "runtime": "nodejs18.x"
    }
  }
}
